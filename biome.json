{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"files": {
		"ignore": [
			"node_modules",
			"out",
			"dist",
			".vscode-test",
			".nyc_output",
			"coverage",
			"**/*.mjs",
			"*.md",
			"*.yml",
			"*.yaml"
		]
	},
	"linter": {
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true
	}
}

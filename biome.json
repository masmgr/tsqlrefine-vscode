{
	"$schema": "https://biomejs.dev/schemas/2.3.11/schema.json",
	"files": {
		"includes": [
			"**",
			"!node_modules",
			"!out",
			"!!dist",
			"!.vscode-test",
			"!.nyc_output",
			"!coverage",
			"!**/*.mjs",
			"!*.md",
			"!*.yml",
			"!*.yaml"
		]
	},
	"linter": {
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true
	}
}
